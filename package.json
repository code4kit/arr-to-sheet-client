{
  "name": "arr-to-sheet-client",
  "version": "1.0.1",
  "description": "arr-to-sheet-server's client. you can install 'arr-to-sheet-client' by yarn or npm. https://github.com/code4kit/arr-to-sheet-server#readme",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./sample.js",
    "commit": "git cz",
    "standard": "semistandard --fix",
    "document": "documentation build -f md ./src/ | add-text-to-markdown ./DOCUMENT.md --section 'DocumentationJS' --write",
    "readme": "npx readme-md-generator",
    "test": "mocha"
  },
  "author": "code4kit",
  "contributors": [
    "waricoma"
  ],
  "license": "MIT",
  "homepage": "https://github.com/code4kit/arr-to-sheet-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4kit/arr-to-sheet-client.git"
  },
  "bugs": {
    "url": "https://github.com/code4kit/arr-to-sheet-client/issues"
  },
  "engines": {
    "yarn": ">=1.19.1",
    "node": ">=12.11.1"
  },
  "dependencies": {
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "add-text-to-markdown": "2.0.0",
    "commitizen": "4.3.0",
    "cz-emoji": "1.2.0",
    "documentation": "12.1.4",
    "mocha": "7.0.0",
    "readme-md-generator": "0.7.0",
    "semistandard": "14.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "semistandard": {
    "global": [
      "describe",
      "it"
    ]
  }
}
